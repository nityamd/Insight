{
  "manifest_version": 2,

  "name": "Hobbes",
  "description": "Execute script after click in popup.html (chrome extension)",
  "version": "1.0",

  "icons": {
    "48": "icon_hobbes.png"
  },

  "permissions": [
    "tabs", "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icon_hobbes.png",
    "default_popup": "popup.html"
  }
}
